<html>
  <head>
    <title></title>
  </head>
  <body>

    <div class="container">

      <%= form_for @users do |f| %>

      <%= f.label :first_name  %>
      <%= f.text_field :first_name %>

      <%= f.label :last_name  %>
      <%= f.text_field :last_name %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.submit("Sign Up", class: 'btn') %>

      <% end %>




    </div>
  </body>
</html>
